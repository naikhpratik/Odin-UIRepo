@model Odin.ViewModels.Mailers.CreatePasswordViewModel

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body style="padding-top: 0px;">
    <div style="top: 0px;  width: 100%;  padding: 0px;  position: fixed;display:block" class="container body-content">
        <div class="row">
            <div style="width: 626px; margin:auto; text-align:center; padding-top:5px; margin-top:80px; padding-bottom:12px; background-color:white; border:1px solid;" class="col mailer">
                <div id="dwellworks" class="mailer" style="margin-top: 40px;text-align:center;display:block">
                    <img src="@Model.LogoUrl" style="vertical-align:middle;text-align:center;border:0">
                    <p style="border-left: 1px solid gray;margin-left:4px;top:-2px;height: 21px;position:relative;display:inline-block;line-height:1.846;">&nbsp;</p>
                    <p style="top:-3px;color:#4bad48;font-size:20px;font-family:sans-serif;line-height:1.846;display:inline-block;margin: 0 0 1em;text-align:center;letter-spacing:0.1px">Destination</p>
                </div>
                
                <div style="margin-top: 72px;font-size: 36px;line-height: 50px;font-family: sans-serif;" id="caption">
                    <span>Create your</span><span id="pw" style="color: #31A4DD;"> Password</span>
                    <div style="width: 344px;margin: auto;font-size:initial;line-height: 22px;margin-top: 14px;" class="mailer">
                        <p>
                            @*Spot for placeholder_text*@
                        </p>
                    </div>
                </div>
                <div id="group-blk" style="margin-top: 60px;padding-top: 0px;" class="mailer">
                    <div style="padding-left:15px;padding-right:15px;" class="form-group">
                        <div style="font-size: 20px;width: 100%;" class="col-md-10;">
                            <a clicktracking="off" style="text-decoration:none;display:inline-block;margin-bottom: 0;font-weight: normal;text-align: center;vertical-align: middle;-ms-touch-action: manipulation;touch-action: manipulation;cursor: pointer;background-image: none;border: 1px solid transparent;white-space: nowrap;padding: 1px 10px;font-size: 20px;line-height: 1.42857143;border-radius: 4px;-webkit-user-select: none;-moz-user-select: none;-ms-user-select: none;user-select: none;color: #ffffff;background-color: #4BAD48; border-color: #95a5a6;" href="@Model.CreateUrl">Create Password</a>
                        </div>

                        <div style="margin-top: 110px;line-height: 10px;font-weight: 500;" class="col-md-10 mailer">
                            <p>© 2017 Dwellworks, LLC. All Rights Reserved.</p>
                            <p>Privacy Policy | Code of Conduct</p>
                        </div>
                    </div>
                </div>
                
                </div>
            </div>
        </div>
    </body>
</html>