                        @Html.LabelFor(model => @Model.Pets.ElementAt(i).Size)
                        <span class="intake-span">@Model.Pets.ElementAt(i).Size</span>
                        @Html.TextBoxFor(m => @Model.Pets.ElementAt(i).Size, new { @class = "form-control intake-input" })
