@model Odin.ViewModels.Orders.Transferee.OrdersTransfereeViewModel

<div class="container-fluid navWrapper">
    <div class="navbar-header transfereeSideNav__person">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <div class="navbar-brand" href="#">
            @*<img class="person" src="~/Content/Images/icn_person.png" />*@
            <p class="eeName">@Model.Transferee.FullName</p>
            @Html.HiddenFor(Model => Model.Transferee.Email, new { id = "TransfereeEmail" })
        </div>
    </div>
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <div class="eeDetails">
            <div class="eeOriginVipWrap">
                <p class="eeDetails eeOrigin">@Model.RelocationType</p>
                @if (Model.IsVipDisplay == "Yes")
                {
                    <p class="eeDetails eeBar">|</p>
                        <p class="eeDetails eeVip">VIP</p>

                }
            </div>
            <p class="eeDetails">@Model.Rmc</p>
            <p class="eeDetails">@Model.Client</p>
        </div>
        <ul class="nav navbar-nav">
            <hr class="transfereeSideNav__rule" />

            <li class="item selected nav-item" data-panel="intake">
                <a id="intake" href="#intake">
                    <img src="~/Content/Images/icn_intake.png" />
                    <label>Program Details</label>
                </a>
            </li>
            <li class="item nav-item" data-panel="details">
                <a id="details" href="#details">
                    <img src="~/Content/Images/icn_details.png" />
                    <label>Service Reporting</label>
                </a>
            </li>
            <li class="item nav-item" data-panel="housing">
                <a id="housing" href="#housing">
                    <img src="~/Content/Images/icn_housing.png" />
                    <label>Housing</label>
                </a>
            </li>
            <li class="item nav-item" data-panel="history">
                <a id="history" href="#history">
                    <img src="~/Content/Images/icn_history.png" />
                    <label>History</label>
                </a>
            </li>
            <hr class="transfereeSideNav__rule" />
            <li class="item nav-item" data-panel="itinerary">
                <a id="itinerary" href="#itinerary">
                    <img src="~/Content/Images/icn_internal_itinerary.png" />
                    <label>Itinerary</label>
                </a>
            </li>
        </ul>
        <span id="backButton">BACK TO LIST</span>
    </div>
</div>




